# Google Gemini API Key (now required via environment variable)
GOOGLE_API_KEY=your_api_key_here

# FastAPI settings
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4

# CORS settings (for production, set to your frontend URL)
CORS_ORIGINS=http://localhost:3000,https://your-netlify-app.netlify.app

# Security (optional)
SECRET_KEY=your_secret_key_here